{
  "name": "@jupiterone/graph-snipe-it",
  "version": "0.3.0",
  "description": "A graph conversion tool for https://snipeitapp.com/",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "collect": "j1-integration collect",
    "graph": "j1-integration visualize",
    "start": "yarn collect; yarn graph",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "prebuild": "yarn test:ci",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@jupiterone/integration-sdk-core": "^1.0.2",
    "@lifeomic/attempt": "^3.0.0",
    "base-64": "^0.1.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-cli": "^1.0.2",
    "@jupiterone/integration-sdk-testing": "^1.0.2",
    "@types/har-format": "^1.2.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "wait-for-expect": "^3.0.2"
  }
}
